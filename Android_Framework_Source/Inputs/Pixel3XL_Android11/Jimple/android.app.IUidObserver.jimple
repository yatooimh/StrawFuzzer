public interface  android.app.IUidObserver extends java.lang.Object implements android.os.IInterface
{

    public abstract void onUidActive(int) throws android.os.RemoteException;

    public abstract void onUidCachedChanged(int, boolean) throws android.os.RemoteException;

    public abstract void onUidGone(int, boolean) throws android.os.RemoteException;

    public abstract void onUidIdle(int, boolean) throws android.os.RemoteException;

    public abstract void onUidStateChanged(int, int, long, int) throws android.os.RemoteException;
}
