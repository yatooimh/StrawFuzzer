public interface  android.app.blob.IBlobStoreManager extends java.lang.Object implements android.os.IInterface
{

    public abstract void abandonSession(long, java.lang.String) throws android.os.RemoteException;

    public abstract void acquireLease(android.app.blob.BlobHandle, int, java.lang.CharSequence, long, java.lang.String) throws android.os.RemoteException;

    public abstract long createSession(android.app.blob.BlobHandle, java.lang.String) throws android.os.RemoteException;

    public abstract void deleteBlob(long) throws android.os.RemoteException;

    public abstract android.app.blob.LeaseInfo getLeaseInfo(android.app.blob.BlobHandle, java.lang.String) throws android.os.RemoteException;

    public abstract java.util.List getLeasedBlobs(java.lang.String) throws android.os.RemoteException;

    public abstract long getRemainingLeaseQuotaBytes(java.lang.String) throws android.os.RemoteException;

    public abstract android.os.ParcelFileDescriptor openBlob(android.app.blob.BlobHandle, java.lang.String) throws android.os.RemoteException;

    public abstract android.app.blob.IBlobStoreSession openSession(long, java.lang.String) throws android.os.RemoteException;

    public abstract java.util.List queryBlobsForUser(int) throws android.os.RemoteException;

    public abstract void releaseLease(android.app.blob.BlobHandle, java.lang.String) throws android.os.RemoteException;

    public abstract void waitForIdle(android.os.RemoteCallback) throws android.os.RemoteException;
}
