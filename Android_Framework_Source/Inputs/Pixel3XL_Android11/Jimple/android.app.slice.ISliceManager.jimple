public interface  android.app.slice.ISliceManager extends java.lang.Object implements android.os.IInterface
{

    public abstract void applyRestore(byte[], int) throws android.os.RemoteException;

    public abstract int checkSlicePermission(android.net.Uri, java.lang.String, java.lang.String, int, int, java.lang.String[]) throws android.os.RemoteException;

    public abstract byte[] getBackupPayload(int) throws android.os.RemoteException;

    public abstract android.net.Uri[] getPinnedSlices(java.lang.String) throws android.os.RemoteException;

    public abstract android.app.slice.SliceSpec[] getPinnedSpecs(android.net.Uri, java.lang.String) throws android.os.RemoteException;

    public abstract void grantPermissionFromUser(android.net.Uri, java.lang.String, java.lang.String, boolean) throws android.os.RemoteException;

    public abstract void grantSlicePermission(java.lang.String, java.lang.String, android.net.Uri) throws android.os.RemoteException;

    public abstract boolean hasSliceAccess(java.lang.String) throws android.os.RemoteException;

    public abstract void pinSlice(java.lang.String, android.net.Uri, android.app.slice.SliceSpec[], android.os.IBinder) throws android.os.RemoteException;

    public abstract void revokeSlicePermission(java.lang.String, java.lang.String, android.net.Uri) throws android.os.RemoteException;

    public abstract void unpinSlice(java.lang.String, android.net.Uri, android.os.IBinder) throws android.os.RemoteException;
}
