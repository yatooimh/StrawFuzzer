public interface  android.bluetooth.IBluetoothHidDeviceCallback extends java.lang.Object implements android.os.IInterface
{

    public abstract void onAppStatusChanged(android.bluetooth.BluetoothDevice, boolean) throws android.os.RemoteException;

    public abstract void onConnectionStateChanged(android.bluetooth.BluetoothDevice, int) throws android.os.RemoteException;

    public abstract void onGetReport(android.bluetooth.BluetoothDevice, byte, byte, int) throws android.os.RemoteException;

    public abstract void onInterruptData(android.bluetooth.BluetoothDevice, byte, byte[]) throws android.os.RemoteException;

    public abstract void onSetProtocol(android.bluetooth.BluetoothDevice, byte) throws android.os.RemoteException;

    public abstract void onSetReport(android.bluetooth.BluetoothDevice, byte, byte, byte[]) throws android.os.RemoteException;

    public abstract void onVirtualCableUnplug(android.bluetooth.BluetoothDevice) throws android.os.RemoteException;
}
