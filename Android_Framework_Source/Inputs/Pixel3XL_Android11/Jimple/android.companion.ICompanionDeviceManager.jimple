public interface  android.companion.ICompanionDeviceManager extends java.lang.Object implements android.os.IInterface
{

    public abstract void associate(android.companion.AssociationRequest, android.companion.IFindDeviceCallback, java.lang.String) throws android.os.RemoteException;

    public abstract void disassociate(java.lang.String, java.lang.String) throws android.os.RemoteException;

    public abstract java.util.List getAssociations(java.lang.String, int) throws android.os.RemoteException;

    public abstract boolean hasNotificationAccess(android.content.ComponentName) throws android.os.RemoteException;

    public abstract boolean isDeviceAssociatedForWifiConnection(java.lang.String, java.lang.String, int) throws android.os.RemoteException;

    public abstract android.app.PendingIntent requestNotificationAccess(android.content.ComponentName) throws android.os.RemoteException;

    public abstract void stopScan(android.companion.AssociationRequest, android.companion.IFindDeviceCallback, java.lang.String) throws android.os.RemoteException;
}
