public interface  android.hardware.lights.ILightsManager extends java.lang.Object implements android.os.IInterface
{

    public abstract void closeSession(android.os.IBinder) throws android.os.RemoteException;

    public abstract android.hardware.lights.LightState getLightState(int) throws android.os.RemoteException;

    public abstract java.util.List getLights() throws android.os.RemoteException;

    public abstract void openSession(android.os.IBinder) throws android.os.RemoteException;

    public abstract void setLightStates(android.os.IBinder, int[], android.hardware.lights.LightState[]) throws android.os.RemoteException;
}
