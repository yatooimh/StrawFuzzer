public interface  android.media.tv.ITvRemoteServiceInput extends java.lang.Object implements android.os.IInterface
{

    public abstract void clearInputBridge(android.os.IBinder) throws android.os.RemoteException;

    public abstract void closeInputBridge(android.os.IBinder) throws android.os.RemoteException;

    public abstract void openGamepadBridge(android.os.IBinder, java.lang.String) throws android.os.RemoteException;

    public abstract void openInputBridge(android.os.IBinder, java.lang.String, int, int, int) throws android.os.RemoteException;

    public abstract void sendGamepadAxisValue(android.os.IBinder, int, float) throws android.os.RemoteException;

    public abstract void sendGamepadKeyDown(android.os.IBinder, int) throws android.os.RemoteException;

    public abstract void sendGamepadKeyUp(android.os.IBinder, int) throws android.os.RemoteException;

    public abstract void sendKeyDown(android.os.IBinder, int) throws android.os.RemoteException;

    public abstract void sendKeyUp(android.os.IBinder, int) throws android.os.RemoteException;

    public abstract void sendPointerDown(android.os.IBinder, int, int, int) throws android.os.RemoteException;

    public abstract void sendPointerSync(android.os.IBinder) throws android.os.RemoteException;

    public abstract void sendPointerUp(android.os.IBinder, int) throws android.os.RemoteException;

    public abstract void sendTimestamp(android.os.IBinder, long) throws android.os.RemoteException;
}
