public interface  android.service.vr.IVrManager extends java.lang.Object implements android.os.IInterface
{

    public abstract boolean getPersistentVrModeEnabled() throws android.os.RemoteException;

    public abstract int getVr2dDisplayId() throws android.os.RemoteException;

    public abstract boolean getVrModeState() throws android.os.RemoteException;

    public abstract void registerListener(android.service.vr.IVrStateCallbacks) throws android.os.RemoteException;

    public abstract void registerPersistentVrStateListener(android.service.vr.IPersistentVrStateCallbacks) throws android.os.RemoteException;

    public abstract void setAndBindCompositor(java.lang.String) throws android.os.RemoteException;

    public abstract void setPersistentVrModeEnabled(boolean) throws android.os.RemoteException;

    public abstract void setStandbyEnabled(boolean) throws android.os.RemoteException;

    public abstract void setVr2dDisplayProperties(android.app.Vr2dDisplayProperties) throws android.os.RemoteException;

    public abstract void unregisterListener(android.service.vr.IVrStateCallbacks) throws android.os.RemoteException;

    public abstract void unregisterPersistentVrStateListener(android.service.vr.IPersistentVrStateCallbacks) throws android.os.RemoteException;
}
