public interface  android.speech.IRecognitionService extends java.lang.Object implements android.os.IInterface
{

    public abstract void cancel(android.speech.IRecognitionListener, java.lang.String, java.lang.String) throws android.os.RemoteException;

    public abstract void startListening(android.content.Intent, android.speech.IRecognitionListener, java.lang.String, java.lang.String) throws android.os.RemoteException;

    public abstract void stopListening(android.speech.IRecognitionListener, java.lang.String, java.lang.String) throws android.os.RemoteException;
}
