public interface  android.view.IScrollCaptureController extends java.lang.Object implements android.os.IInterface
{

    public abstract void onCaptureBufferSent(long, android.graphics.Rect) throws android.os.RemoteException;

    public abstract void onCaptureStarted() throws android.os.RemoteException;

    public abstract void onClientConnected(android.view.IScrollCaptureClient, android.graphics.Rect, android.graphics.Point) throws android.os.RemoteException;

    public abstract void onClientUnavailable() throws android.os.RemoteException;

    public abstract void onConnectionClosed() throws android.os.RemoteException;
}
