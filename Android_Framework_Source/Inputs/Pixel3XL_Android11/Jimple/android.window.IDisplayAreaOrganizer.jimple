public interface  android.window.IDisplayAreaOrganizer extends java.lang.Object implements android.os.IInterface
{

    public abstract void onDisplayAreaAppeared(android.window.DisplayAreaInfo, android.view.SurfaceControl) throws android.os.RemoteException;

    public abstract void onDisplayAreaInfoChanged(android.window.DisplayAreaInfo) throws android.os.RemoteException;

    public abstract void onDisplayAreaVanished(android.window.DisplayAreaInfo) throws android.os.RemoteException;
}
